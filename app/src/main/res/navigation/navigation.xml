<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="android.latihan.my_finance.homeFragment"
        android:label="My Finance"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_setting"
            app:destination="@id/settingFrag"/>
        <action android:id="@+id/action_homeFragment_to_about"
            app:destination="@id/aboFrag"/>
        <action android:id="@+id/action_homeFragment_to_Login"
            app:destination="@id/loginAtention"/>
    </fragment>
    <fragment
        android:id="@+id/settingFrag"
        android:name="android.latihan.my_finance.settingFragment"
        android:label="Setting"
        tools:layout="@layout/fragment_setting">
    </fragment>
    <fragment
        android:id="@+id/aboFrag"
        android:name="android.latihan.my_finance.aboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about">
    </fragment>
    <fragment
        android:id="@+id/loginAtention"
        android:label="Account"
        android:name="android.latihan.my_finance.loginFragment"
        tools:layout="@layout/fragment_account_sign">

    </fragment>
</navigation>